/*
 * SKY130 High Density Standard Cell Library
 * Timing Model: Slow Corner (ss_n40C_1v60)
 */

library (sky130_fd_sc_hd__ss_n40C_1v60) {
  technology (cmos);
  delay_model : table_lookup;
  time_unit : "1ns";
  voltage_unit : "1V";
  current_unit : "1mA";
  
  nom_process : 1.0;
  nom_voltage : 1.60;
  nom_temperature : -40.0;
  
  default_cell_leakage_power : 0.002;
  
  cell (sky130_fd_sc_hd__inv_1) {
    area : 1.0;
    pin(A) { direction : input; capacitance : 0.005; }
    pin(Y) { 
      direction : output;
      function : "!A";
      timing() {
        related_pin : "A";
        cell_rise(scalar) { values("0.20"); }
        cell_fall(scalar) { values("0.20"); }
      }
    }
  }
  
  cell (sky130_fd_sc_hd__buf_1) {
    area : 2.0;
    pin(A) { direction : input; capacitance : 0.005; }
    pin(X) { 
      direction : output;
      function : "A";
      timing() {
        related_pin : "A";
        cell_rise(scalar) { values("0.25"); }
        cell_fall(scalar) { values("0.25"); }
      }
    }
  }
  
  cell (sky130_fd_sc_hd__dfxtp_1) {
    area : 6.0;
    ff(IQ, IQN) {
      clocked_on : "CLK";
      next_state : "D";
    }
    pin(CLK) { direction : input; capacitance : 0.008; clock : true; }
    pin(D) { 
      direction : input; 
      capacitance : 0.005;
      timing() {
        related_pin : "CLK";
        timing_type : setup_rising;
        rise_constraint(scalar) { values("0.4"); }
        fall_constraint(scalar) { values("0.4"); }
      }
    }
    pin(Q) { 
      direction : output;
      timing() {
        related_pin : "CLK";
        timing_type : rising_edge;
        cell_rise(scalar) { values("0.50"); }
        cell_fall(scalar) { values("0.50"); }
      }
    }
  }
}