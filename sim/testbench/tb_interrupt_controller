    %pad/s 16;
    %store/vec4 v0x559cf3534a60_0, 0, 16;
    %vpi_func 3 295 "$random" 32 {0 0 0};
    %store/vec4 v0x559cf35348f0_0, 0, 32;
    %vpi_func 3 296 "$random" 32 {0 0 0};
    %pad/s 1;
    %store/vec4 v0x559cf35345b0_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0x559cf3534820_0;
    %cmpi/e 1, 1, 1;
    %jmp/0xz  T_7.2, 6;
    %vpi_call/w 3 300 "$display", "[FAIL] interrupt_req is X" {0 0 0};
T_7.2 ;
    %load/vec4 v0x559cf35346b0_0;
    %cmpi/e 4294967295, 4294967295, 32;
    %jmp/0xz  T_7.4, 6;
    %vpi_call/w 3 303 "$display", "[FAIL] interrupt_cause is X" {0 0 0};
T_7.4 ;
    %jmp T_7.0;
T_7.1 ;
    %pop/vec4 1;
    %vpi_call/w 3 306 "$display", "Stress test: No X values" {0 0 0};
    %delay 10000, 0;
    %vpi_call/w 3 309 "$display", "\012=====================================" {0 0 0};
    %vpi_call/w 3 310 "$display", "Test Summary: %0d/%0d tests passed", v0x559cf35349c0_0, v0x559cf3534da0_0 {0 0 0};
    %load/vec4 v0x559cf35349c0_0;
    %load/vec4 v0x559cf3534da0_0;
    %cmp/e;
    %jmp/0xz  T_7.6, 4;
    %vpi_call/w 3 312 "$display", "=== ALL TESTS PASSED! ===" {0 0 0};
    %jmp T_7.7;
T_7.6 ;
    %vpi_call/w 3 314 "$display", "=== SOME TESTS FAILED ===" {0 0 0};
T_7.7 ;
    %vpi_call/w 3 316 "$display", "=====================================" {0 0 0};
    %delay 100000, 0;
    %vpi_call/w 3 318 "$finish" {0 0 0};
    %end;
    .thread T_7;
